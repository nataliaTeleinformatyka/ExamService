{% extends 'base.html' %}

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}

{% block body %}
    <div class="container">
        <div class="studentExam">
            {{ error }}
            {% for action in data %}
                {% if information==true %}
                <p><a href="{{ path('studentExamStartInfo', { 'userExamId': action.user_exam_id}) }}">
                        {{ action.exam_name|default('') }} {{ action.access_period|default('') }}
                        {{ action.remaining_attempts|default('') }}</a></p>
                    </a></p>
                {% endif %}
            {% endfor %}

        </div>
    </div>
{% endblock %}